<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="0.0" minWidth="0.0" prefHeight="271.0" prefWidth="369.0"
            styleClass="roundedBordersDialog" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/17.0.12"
            fx:controller="org.dpsoftware.gui.controllers.ProfileDialogController">

    <GridPane alignment="CENTER" prefHeight="271.0" prefWidth="370.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="204.66666666666666" minWidth="10.0" prefWidth="188.4" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="141.99999999999997" minWidth="10.0" prefWidth="141.99999999999997" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints fx:id="cpuRow" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints fx:id="gpuRow" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="15.0" />
        </padding>
        <Label fx:id="cpuLabel" text="%fxml.profile.cpu" wrapText="true" GridPane.rowIndex="4"/>
        <Label text="%fxml.profile.process2" wrapText="true" GridPane.rowIndex="2"/>
        <Label text="%fxml.profile.process3" wrapText="true" GridPane.rowIndex="3"/>
        <Label text="%fxml.profile.process1" wrapText="true" GridPane.rowIndex="1"/>
        <Label fx:id="gpuLabel" text="%fxml.profile.gpu" wrapText="true" GridPane.rowIndex="5"/>
        <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="6" GridPane.valignment="BOTTOM">
            <Button fx:id="okButton" minWidth="60.0" mnemonicParsing="false" onMouseClicked="#saveAndClose" text="%fxml.dialog.ok">
                <HBox.margin>
                    <Insets top="10.0" />
                </HBox.margin>
            </Button>
        </HBox>
        <HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="6">
            <Button fx:id="cancelButton" contentDisplay="TOP" minWidth="60.0" mnemonicParsing="false" onMouseClicked="#close" prefHeight="25.0" text="%fxml.dialog.cancel">
                <HBox.margin>
                    <Insets top="10.0" />
                </HBox.margin>
            </Button>
        </HBox>
        <ComboBox fx:id="process1" editable="true" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <ComboBox fx:id="process2" editable="true" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <ComboBox fx:id="process3" editable="true" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <CheckBox fx:id="enableFullScreenDetection" mnemonicParsing="false" GridPane.columnIndex="1"/>
        <Label text="%fxml.profile.fullscreen.cb"/>
        <ComboBox fx:id="cpuThreshold" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <ComboBox fx:id="gpuThreshold" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
    </GridPane>
</AnchorPane>
